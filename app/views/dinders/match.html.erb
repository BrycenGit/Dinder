


  <h1>It's a Match!</h1>

  <div class="modal show" id="match_alert" tabindex="-1" role="dialog" aria-labelledby="match_alert_title" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="match_alert_title">You Have a Match with A Friend!</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <h1>Restaurant Name</h1>
          <p>Restaurant Image</p>
          <p>Restaurant Address</p>
          <a>Restaurant Website</a>
        </div>
        <div class="modal-footer">
          <%= link_to "Home", root_path %>
        </div>
      </div>
    </div>
  </div>
</body>

